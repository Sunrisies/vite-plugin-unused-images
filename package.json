{
  "name": "vite-plugin-unused-images",
  "description": "A Vite plugin to detect and report unused image assets in your project",
  "version": "0.0.4",
  "author": "sunrise",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.1",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3",
    "vite": "^4.3.3"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.3",
    "chalk": "^5.6.0",
    "glob": "^11.0.3",
    "lightningcss": "^1.30.1",
    "rollup": "^4.40.0"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
