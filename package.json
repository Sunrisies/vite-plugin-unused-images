    "clean",
    "frontend",
    "optimization",
    "webpack-alternative"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Sunrisies/vite-plugin-unused-images.git"
  },
  "bugs": {
    "url": "https://github.com/Sunrisies/vite-plugin-unused-images/issues"
